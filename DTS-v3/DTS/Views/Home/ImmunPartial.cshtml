

<table style="font-size:medium;font-family:'Palatino Linotype'" class="table table-bordered">
    <tr>
        <th>
           Location
        </th>
        <th>
           Numb Res Comm
        </th>
        <th>
           Numb Res Immun
        </th>
        <th>
           Numb Res Not Immun
        </th>
        <th>
           Per Res Immun
        </th>
        <th>
           Per Res Not Immun
        </th>
    </tr>
    @{
        string[] names = ViewBag.Names;
        var list = (List<DTS.Models.Immunization>)ViewBag.List;
        foreach (var item in list)
        {
        <tr>
            <td>
                @names[item.Location - 1]
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Numb_Res_Comm)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Numb_Res_Immun)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Numb_Res_Not_Immun)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Per_Res_Immun)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Per_Res_Not_Immun)
            </td>
            <td>
                <div style="width:300px;margin-top:2px">
                    @Html.ActionLink("Edit", "Edit_Immun", "Edit", new { id = item.Id }, new { @class = "b" })
                    @Html.ActionLink("Details", "Immun_Details", "Details", new { id = item.Id }, new { @class = "b" })
                    @Html.ActionLink("Delete", "Immun_Delete", "Delete", new { id = item.Id }, new { @class = "b" })
                </div>
            </td>
        </tr>
        }
    }

</table>
<style>
    .b {
        border: 1px solid blue;
        border-radius: 6px;
        color: darkred;
        text-decoration: none;
        padding: 6px;
        font-size:medium;
    }

        .b:hover {
            color: red;
            text-decoration: none;
            background-color: blue;
        }
</style>





